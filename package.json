{
  "name": "node-typescript-express-postgresql-authentication-authorization",
  "version": "1.0.0",
  "description": "Simple example of how to authenticate and authorize with jsonwebtoken",
  "keywords": [
    "express authentication authorization",
    "jsonwebtoken authentication",
    "jwt authentication",
    "postgresql authentication authorization"
  ],
  "scripts": {
    "start": "node dist/server",
    "build": "tsc",
    "dev": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint src --ext .ts",
    "test": "createdbjs test_db_test --user=$POSTGRES_USER ; npm run build ; ENV=test jasmine-ts ; dropdbjs test_db_test --user=$POSTGRES_USER"
  },
  "author": "Al Diaz",
  "license": "MIT",
  "main": "server.ts",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.6.1",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "pgtools": "^0.3.2",
    "prettier": "^2.3.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
